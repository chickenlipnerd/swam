<!DOCTYPE html>
<html lang="en">
    <head>
        @RenderPage("~/Shared/_TopHead.cshtml")
        @RenderPage("~/Shared/_FlexSlider.cshtml")
        @RenderPage("~/Shared/_Html5Shim.cshtml")
    </head>

    <body class="@Global.BodyClass">

        @RenderPage("~/Shared/Layout/_Header.cshtml")

        <div id="content-top-wrap">
            <div class="container">
                @RenderSection("home_slider", required: false)
            </div>
            <div class="flex-img">
                <img src="images/home-slider-bg.jpg" alt="this is a featured swim">
            </div>
        </div>

        @RenderSection("featured", required: false)
        @RenderBody()
        @RenderPage("~/Shared/Layout/_Footer.cshtml")
        @RenderSection("Scripts", required: false)
    </body>
</html>
